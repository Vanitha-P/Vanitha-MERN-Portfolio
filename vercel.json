{
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/client/dist/$1"
      },
      {
        "src": "/mail(.*)",
        "dest": "./index.js"
      }
    ]
  }
  